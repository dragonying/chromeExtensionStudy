{
    "name": "CustChromePlug",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Customize Google plug-in demo !",
    "icons": {
        "16": "icons/yt.png",
        "32": "icons/yt.png",
        "48": "icons/yt.png",
        "64": "icons/yt.png",
        "128": "icons/yt.png"
    },
    "action": {
        "default_title": "CustChromePlug popup",
        "default_popup": "action/popup.html",
        "default_icon": {
            "16": "icons/yt.png",
            "32": "icons/yt.png",
            "48": "icons/yt.png",
            "64": "icons/yt.png",
            "128": "icons/yt.png"
        }
    },
    "options_page": "options/test.html",
    "background": {
        "service_worker": "background/test.js"
    },
    "content_scripts": [
        {
            "matches": [
                "*://blog.csdn.net/*"
            ],
            "js": [
                "contentScript/js/test.js"
            ],
            "css": [
                "contentScript/css/test.css"
            ],
            "run_at": "document_end"
        }
    ],
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "notifications",
        "declarativeContent"
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "action/image/*"
            ],
            "matches": [
                "http://*/*",
                "https://*/*"
            ]
        }
    ]
}